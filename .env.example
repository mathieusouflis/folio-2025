# Database Configuration
# Production database URL
DATABASE_URI="your_production_database_url"

# Development database URL
DATABASE_DEV_URI="your_development_database_url"

# Payload CMS Configuration
# Secret key for Payload CMS (generate a secure random string)
PAYLOAD_SECRET="your_payload_secret_key"

# Environment
# Options: development, production, test
NODE_ENV="development"

# Vercel Blob Storage (optional)
# Token for Vercel Blob storage if you're using it for media
BLOB_NEXT_READ_WRITE_TOKEN="your_vercel_blob_token"
